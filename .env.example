# .env.example - Template for local development environment
# Copy to .env for local debugging with VS Code

# Application settings
PROJECT_NAME=Airweave
LOCAL_DEVELOPMENT=true
LOCAL_CURSOR_DEVELOPMENT=true
FRONTEND_LOCAL_DEVELOPMENT_PORT=8085
DTAP_ENVIRONMENT=local

# Database settings - will connect to Docker services via localhost
POSTGRES_DB=airweave
POSTGRES_PASSWORD=airweave1234!
POSTGRES_HOST=localhost
POSTGRES_USER=airweave

# Redis settings
REDIS_HOST=localhost

# Vector database settings
QDRANT_HOST=localhost
QDRANT_PORT=6333

# Embedding model settings
TEXT2VEC_INFERENCE_URL=http://localhost:9878

# Feature flags
RUN_DB_SYNC=true
RUN_ALEMBIC_MIGRATIONS=true
LOCAL_CURSOR_DEVELOPMENT=true

# Default admin user
FIRST_SUPERUSER=admin@example.com
FIRST_SUPERUSER_PASSWORD=changeme123!

# Security - will be auto-generated if not provided

# DO NOT add real keys to this example file
ENCRYPTION_KEY=

# API Keys - required for certain features
# DO NOT add real keys to this example file
OPENAI_API_KEY=
MISTRAL_API_KEY=
